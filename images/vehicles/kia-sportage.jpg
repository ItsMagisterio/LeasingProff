    function getCookie(cookieName) {
        var cookieValue = undefined;
        if (document.cookie) {
            var value = document.cookie.match('(^|;| )' + cookieName + '=([^;]*)(;|$)');
            if (value) {
                cookieValue = value[2];
            }
        }
        return cookieValue;
    }

