<script type="text/javascript">
    $(document).ready(function ()
    {
        let jqBody = $("body");
        jqBody.on("click", ".video-trigger", function ()
        {
            let video = $(this).children().clone();
            let jqVideo = $(video);

            jqVideo.attr("width", "750");
            jqVideo.attr("height", "500");

            $("#topplayer-content").html(video);
        });

        jqBody.on("click", ".video-image-trigger", function ()
        {
            let content = $("#topplayer-content");
            content.html("");

            let videoChild = $(this).children();
            let videoUrl = videoChild.attr("data");
            if (videoUrl == undefined)
            {
                videoUrl = videoChild.find("img").attr("data");
            }

            let iframe = document.createElement('iframe');
            iframe.width = '750';
            iframe.height = '422';
            iframe.src = videoUrl;
            iframe.frameborder = '0';
            iframe.setAttribute('allowFullScreen', '');
            iframe.setAttribute('allow', 'autoplay');

            content.html(iframe);
        });

        jqBody.on("click", ".more-trigger", function ()
        {
            let jqThis = $(this);
            let content = jqThis.next().clone();

            $(content).show();

            let modalTitle = jqThis.attr('modalTitle');
            $("#topplayer-title").html(modalTitle ? modalTitle : jqThis.text());
            $("#topplayer-content").html(content);
        });

        jqBody.on("click", ".info-trigger", function ()
        {
            let contentId = $(this).attr("rel");
            let container = $("#" + contentId);

            let title = container.find(".title").html();
            let content = container.find(".content").html();

            $("#topplayer-title").html(title);
            $("#topplayer-content").html(content);
        });

        jqBody.on("click", ".info-close", function ()
        {
            $("#topplayer-title").html("");
            $("#topplayer-content").html("");
        });

        if ($('#cookie-popup-info').length !== 0)
        {
            let container = $('#cookie-popup-info');
            let cookieKey = container.find(".cookie-key").html();

            if ($.cookie('euroncap_cookie_key') != cookieKey)
            {
                jQuery.cookie('euroncap_cookie_key', cookieKey, { expires: 365 });

                let title = container.find(".title").html();
                let content = container.find(".content").html();

                $("#topplayer-title").html(title);
                $("#topplayer-content").html(content);

                $('.toplayer-overlay').fadeIn('fast');
                $('.toplayer-container').fadeIn('fast');
            }
        }

        if ($('#timed-popup-info').length !== 0){
            let container = $('#timed-popup-info');
            let title = container.find(".title").html();
            let content = container.find(".content").html();
            let firstCountdown = container.find(".first-countdown").html();  // in seconds
            let subsequentCountdowns = container.find(".subsequent-countdowns").html();  // in minutes
            let startTimeStorageKey = 'startTime';
            let dialogTimeShownStorageKey = 'dialogTimeShown';

            if (!localStorage.getItem(startTimeStorageKey)) {
                localStorage.setItem(startTimeStorageKey, Date.now());
            }

            // Function to calculate the time spent on the page
            function logTimeSpent() {
                const startTime = localStorage.getItem(startTimeStorageKey);

                if (startTime) {
                    const currentTime = Date.now();
                    const timeSpent = Math.floor((currentTime - startTime) / 1000); // Time spent in seconds
                    const dialogShown = localStorage.getItem(dialogTimeShownStorageKey);

                    if (timeSpent >= firstCountdown && (!dialogShown || (currentTime - dialogShown > subsequentCountdowns * 60 * 1000))) {
                        // Store the current timestamp in sessionStorage when the dialog is shown
                        localStorage.setItem(dialogTimeShownStorageKey, currentTime.toString());

                        $("#topplayer-title").html(title);
                        $("#topplayer-content").html(content);

                        $('.toplayer-overlay').fadeIn('fast');
                        $('.toplayer-container').fadeIn('fast');

                    }
                }
            }

            // Log time every 5 seconds for demonstration (you can adjust this interval)
            setInterval(logTimeSpent, 5000);
        }
    });
</script>


